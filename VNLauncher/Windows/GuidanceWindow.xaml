<Window x:Class="VNLauncher.Windows.GuidanceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VNLauncher" Icon="/VNLauncher;component/Resources/windowIcon.ico"
        mc:Ignorable="d" Loaded="GuidanceWindow_Loaded" Top="40" WindowStyle="None"
        Title="" Height="250" Width="400" Background="{StaticResource backgroundColor_Dark}"  ResizeMode="NoResize" Topmost="True">
    <Grid x:Name="mainGrid" Visibility="Visible">
        <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
